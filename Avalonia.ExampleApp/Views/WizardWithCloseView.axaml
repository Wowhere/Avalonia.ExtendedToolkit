<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             xmlns:Controls="clr-namespace:Avalonia.ExtendedToolkit.Controls;assembly=Avalonia.ExtendedToolkit"
             x:Class="Avalonia.ExampleApp.Views.WizardWithCloseView"
             >
  <Grid>
      <Controls:Wizard FinishButtonClosesWindow="True">
        <Controls:WizardPage Title="Hallo" PageType="Exterior"/>
        <Controls:WizardPage Title="World" PageType="Exterior" CanFinish="True"/>
      </Controls:Wizard>
  </Grid>
</UserControl>
